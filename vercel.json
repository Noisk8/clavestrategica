{
  "version": 2,
  "routes": [
    {
      "src": "/api/candidates/bulk-update",
      "dest": "/api/bulk-update.js"
    },
    {
      "src": "/api/candidates/(.*)",
      "dest": "/api/candidates.js"
    },
    {
      "src": "/api/candidates",
      "dest": "/api/candidates.js"
    },
    {
      "src": "/api/export",
      "dest": "/api/export.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}